<%= form_with model: @user, url: "/profile", local: true do |f| %>
  
  <%= f.label :email %>
  <%= f.email_field :email, class: 'form-control mb-3' %>

  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control mb-3' %>

  <%= f.label :image %>
  <%= f.file_field :image,class: 'form-control mb-3', accept: 'image/*' %>
  <%= f.hidden_field :image_cache %>

  <%= f.submit "更新", class: "btn btn-primary"%>
<%end%>