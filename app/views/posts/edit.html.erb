<h1>投稿</h1>
<%= form_with model:@post do |f| %>
 <p><%= f.label :body %><br>
  <%= f.text_area :body %> #bodyの入力フォーム生成
 </p>
 <%= f.label :image %>
 <%= f.file_field :image, multiple: true %> 
 <%= f.hidden_field :image_cache %><br>

 <%= f.collection_check_boxes(:lines_tag_ids, LinesTag.all, :id, :name) do |line_tag| %>
  <%= line_tag.label class: 'form-check-label' do %>
  <%= line_tag.check_box class: 'form-check-input' %>
  <%= line_tag.text %>
  <br>
<% end %>
<% end %>


 <p> <%= f.submit %> #送信ボタンを生成</p>
<% end %>
